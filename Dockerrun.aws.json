{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
        {
            "environment": [
                {
                    "name": "MYSQL_DATABASE",
                    "value": "scheduler"
                },
                {
                    "name": "MYSQL_ROOT_PASSWORD",
                    "value": "secret"
                },
                {
                    "name": "MYSQL_USER",
                    "value": "brunoba"
                },
                {
                    "name": "MYSQL_PASSWORD",
                    "value": "password"
                }
            ],
            "essential": true,
            "image": "mysql:5.7",
            "name": "mysql"
        },
        {
            "essential": true,
            "image": "nginx:1.15",
            "mountPoints": [
                {
                    "containerPath": "/usr/share/nginx/scheduler",
                    "sourceVolume": "_"
                },
                {
                    "containerPath": "/etc/nginx/conf.d/default.conf",
                    "sourceVolume": "_DockerNginxConf_DDefault_Conf"
                }
            ],
            "name": "nginx",
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 8080
                }
            ],
            "workingDirectory": "/usr/share/nginx/scheduler"
        },
        {
            "essential": true,
            "image": "brunoba93/php_fpm_laravel",
            "mountPoints": [
                {
                    "containerPath": "/usr/share/nginx/scheduler",
                    "sourceVolume": "_"
                }
            ],
            "name": "php_fpm",
            "workingDirectory": "/usr/share/nginx/scheduler"
        }
    ],
    "family": "",
    "volumes": [
        {
            "host": {
                "sourcePath": "./"
            },
            "name": "_"
        },
        {
            "host": {
                "sourcePath": "./docker/nginx/conf.d/default.conf"
            },
            "name": "_DockerNginxConf_DDefault_Conf"
        }
    ]
}
